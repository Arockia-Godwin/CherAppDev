        </div>
        <% if @friend_requests.any? %>
        <% @friend_requests.each do |friend_request| %>
        <div class="card card-body end-cards">
          <div class="card-contents">
            <div class="cardprofile-img col-md-1">
              <%= image_tag(friend_request.requester.profile_image, class: 'end-profile mt-1', style:'display: inline-block;') %>
            </div>
            <div class="end-cardtxt col-md-8">
              <p class="top-cardpro pl-2 mb-0"><%= friend_request.requester.full_name %></p>
              <span class="left-hrs pl-2"><%= distance_of_time_in_words(friend_request.created_at, Time.now) %></span>
              <!-- <p class="right-cardtxt pl-2">You have 2 mutual friends</p> -->
              <%= link_to 'Contact', friend_request_path(friend_request, friend_request: { status: :approved }), method: :put, class: 'btn btn-primary card-bttn mt-1 ml-1' %>
            </div>
            <div class="col-md-3">
              <!-- <span class="left-hrs"><%#= distance_of_time_in_words(friend_request.created_at, Time.now) %></span> -->
            </div>
          </div>
        </div>
        <% end %>
