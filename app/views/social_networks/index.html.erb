      <div class="col-md-7 main-cards feed-top-sgm">
        <div class="head-txt">
          <p class="head-txt head-txt1 pt-3 mb-0 justify-content-start friends-mid-seg">
            Friends activities
          </p>
        </div>
        <% if @friends.any? %>
        <% @friends.each do |friend| %>
        <% @flagged_properties = friend.flagged_properties_data %>
        <% if @flagged_properties.any? %>
        <div class="feed-card mt-2">
         
          <div class="card tp-card-sgm">
            <div class="card-body">
              <div class="profile-details d-flex">
                <div class="profile-img">
                  <%= image_tag(friend.profile_image, style: 'border-radius: 50%;', alt: t('img_alts.user_profile'), 'data-action': 'error->users-profile-picture#setDefaultPicture') %>
                </div>
                <div class="profile-txt ml-2">
                  <p class="card-protxt mb-0 mt-2"><%= friend.first_name%></p>
                  <% @flagged_properties.each do |prop| %>
                  <% flag_created_at = FlaggedProperty.find_by(property_id: prop.dig("listingId")).created_at %>
                  <small class="text-muted mute-txt"><%= distance_of_time_in_words(flag_created_at, Time.now)%></small>
                  <% end %>
                </div>
              </div>
              <div class="main-head">
                <p class="para-txt">Saved this house</p>
              </div>
              <div
                id="carouselExampleIndicators"
                class="carousel slide"
                data-ride="carousel"
              >
                <ol class="carousel-indicators">
                  <li
                    data-target="#carouselExampleIndicators"
                    data-slide-to="0"
                    class="active"
                  ></li>
                  <li
                    data-target="#carouselExampleIndicators"
                    data-slide-to="1"
                  ></li>
                  <li
                    data-target="#carouselExampleIndicators"
                    data-slide-to="2"
                  ></li>
                </ol>
                <div class="carousel-inner">
                  
                  <% @flagged_properties.each do |prop| %>
                  <div class="carousel-item active">
                    <%= image_tag(sanitize_image(prop.dig("photos")[0])) %>
                    <%#= link_to image_tag(sanitize_image(prop.dig("photos")[0])), alt: t('look_around.featured.alts.home'), property_path(item['listingId']) %>
                  </div>
                  <div class="carousel-item">
                    <%= image_tag("myfriends/feed/house_image_1.png", class: 'd-block w-100', style:'display: inline-block;') %>
                  </div>
                  <div class="carousel-item">
                    <%= image_tag("myfriends/feed/house_image_1.png", class: 'd-block w-100', style:'display: inline-block;') %>
                  </div>
                  <% end %>
                </div>
              </div>
            </div>
          </div>
        </div>
        <% end %>
        <% end %>
