          <div class="card-contents">
            <div class="cardprofile-img ">
              <%#= image_tag("myfriends/feed/profile_image_2.png", class: 'end-profile1', style:'display: inline-block;') %>
              <%= image_tag(agent.profile_image, class: 'end-profile mt-1', style:'display: inline-block;') %>
            </div>
            <div class="end-cardtxt">
              <p class="right-cardpro mb-0"><%= agent.full_name %></p>
              <p class="right-cardtxt">Cher Prime Agent</p>
              <%#= link_to 'Contact', friend_requests_path(friend_request: { requester_id: current_user.id, requestee_id: agent.id, status: :pending }), method: :post, class: 'btn btn-primary card-bttn' %>
              <% @friends = FriendRequest.find_by(requester_id: current_user.id, requestee_id: agent.id, status: :pending) %>
              <% if @friends %>
              <button type="button" class="btn btn-primary card-bttn mt-2" disabled>Contacted</button>
              <% else %>
              <%= link_to 'Contact', friend_requests_path(friend_request: { requester_id: current_user.id, requestee_id: agent.id, status: :pending }), method: :post, class: 'btn btn-primary card-bttn mt-2' %>
              <% end %>
            </div>
          </div>
        </div>
        <% end %>
        <% end %>
