<%= form_with(model: @room, remote: true, class: "d-flex" ) do |f| %>
  <%= f.text_field :name, class: "form-control mr-2", autocomplete: 'off' %>
  <%= f.submit data: { disable_with: false }, style: 'width:50%;background-color: #3154f5;', class: 'btn btn-success', value: 'Create Group' %>
<% end %>